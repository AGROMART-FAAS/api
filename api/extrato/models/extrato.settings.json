{
  "kind": "collectionType",
  "collectionName": "extratos",
  "info": {
    "name": "extrato",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "itens": {
      "type": "json",
      "required": true
    },
    "valor": {
      "type": "float",
      "required": true
    },
    "entregue": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "tipo_de_entrega": {
      "type": "enumeration",
      "enum": [
        "Receber",
        "Buscar"
      ]
    },
    "loja": {
      "model": "loja",
      "via": "extratos"
    },
    "user": {
      "via": "extratos",
      "plugin": "users-permissions",
      "model": "user"
    },
    "pagamento_realizado": {
      "type": "boolean",
      "default": false
    }
  }
}
